                      rdfs:label "prob_school_transfer" ;
                      
                      rdfs:comment "Relationship that represents the probability of renting a transfer as a service in an specific teaching school." ;
                      
                      rdfs:domain :TeachingSchool ;
                      
                      rdfs:range xsd:decimal .



###  http://www.semanticweb.org/valraiso/ontologies/2024/1/esfvel_ontology/start_time

:start_time rdf:type owl:DatatypeProperty ;
            
            rdfs:label "start_time" ;
            
            rdfs:comment "Relation that states the start time of a scheduled GroupLessonOcurrence instance. These were grouped by week, school and type of group lesson, and the minimum date for that combination is the one represented." ;
            
            rdfs:domain :GroupLesson ,
                        :GroupLessonOcurrence ,
                        :Season ;
            
            rdfs:range xsd:dateTimeStamp .



###  http://www.semanticweb.org/valraiso/ontologies/2024/1/esfvel_ontology/student_birthday

:student_birthday rdf:type owl:DatatypeProperty ;
                  
                  rdfs:domain :Student ;
                  
                  rdfs:range xsd:dateTime ;
                  
                  rdfs:subPropertyOf owl:topDataProperty .



###  http://www.semanticweb.org/valraiso/ontologies/2024/1/esfvel_ontology/student_name

:student_name rdf:type owl:DatatypeProperty ;
              
              rdfs:label "student_name" ;
              
              rdfs:comment "Complete name of a student which had a record in the table of custom orders." ;
              
              rdfs:domain :Student ;
              
              rdfs:range xsd:string .



###  http://www.semanticweb.org/valraiso/ontologies/2024/1/esfvel_ontology/ts_discipline_avg

:ts_discipline_avg rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   
                   rdfs:label "ts_discipline_avg" ;
                   
                   rdfs:comment "States the average that links a teaching school instance and a discipline instance which are related through a TeachingSchoolDisciplineAvgR complex instance (reification)." ;
                   
                   rdfs:domain :TeachingSchoolDisciplineAvgR ;
                   
                   rdfs:range xsd:decimal .





