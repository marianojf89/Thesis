                                                                owl:onProperty :teaches_in ;
                                                                owl:hasValue :EN
                                                              ]
                                                            )
                                       ] ;
                   
                   rdfs:subClassOf :Instructor .



###  http://www.semanticweb.org/valraiso/ontologies/2024/1/esfvel_ontology/EnglishLesson

:EnglishLesson rdf:type owl:Class ;
               
               owl:equivalentClass [ rdf:type owl:Class ;
                                     owl:intersectionOf ( :Lesson
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :given_in_language ;
