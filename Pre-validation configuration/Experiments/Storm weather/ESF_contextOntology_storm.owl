###  http://www.semanticweb.org/valraiso/ontologies/2024/1/esfvel_ontology/end_time

:end_time rdf:type owl:DatatypeProperty ;
          
          rdfs:label "end_time" ;
          
          rdfs:comment "Relation that states the end time of a scheduled GroupLessonOcurrence instance. These were grouped by week, school and type of group lesson, and the maximum date for that combination is the one represented." ;
          
          rdfs:domain :GroupLesson ,
                      :GroupLessonOcurrence ,
                      :Season ;
          
          rdfs:range [ rdf:type rdfs:Datatype ;
                       owl:onDatatype xsd:time ;
                       owl:withRestrictions ( [ xsd:maxInclusive "12:00:00"^^xsd:time
                                              ]
                                            )
                     ] .



